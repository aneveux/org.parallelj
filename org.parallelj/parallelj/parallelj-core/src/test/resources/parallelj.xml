<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:parallelj xmlns:ns2="parallelj">
    <ns2:procedures>
        <procedure capacity="5" name="pr1"/>
        <procedure capacity="10" name="pr2"/>
    </ns2:procedures>
    <ns2:servers>
        <telnet port="10000" host="localhost"/>
        <jmx port="9000" host="localhost"/>
        <ns2:beans>
            <bean class="org.parallelj.sample.programs.MyFirstProgram"/>
            <bean class="org.parallelj.sample.programs.MyLocalProgram"/>
            <bean class="org.parallelj.sample.programs.MyProgram"/>
            <bean class="org.parallelj.sample.programs.multi.MySimpleProcess"/>
        </ns2:beans>
    </ns2:servers>
</ns2:parallelj>
